<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="399.0" prefWidth="803.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EditResourceController">
   <top>
      <Label text="Edit Resource" BorderPane.alignment="CENTER">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
   </top>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Label text="Resource ID" />
                  <Label fx:id="lblResID" text="RESOURCE_ID" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Label text="Title" />
                  <TextField fx:id="txtTitle" prefHeight="25.0" prefWidth="190.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Label text="Year" />
                  <TextField fx:id="txtYear" prefHeight="25.0" prefWidth="190.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <VBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Label text="Book">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label fx:id="lblAuthor" text="Author" />
                              <Region HBox.hgrow="ALWAYS" />
                              <TextField fx:id="txtAuthor" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label fx:id="lblPublisher" text="Publisher" />
                              <Region HBox.hgrow="ALWAYS" />
                              <TextField fx:id="txtPublisher" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label fx:id="lblBookLanguage" text="Language" />
                              <Region HBox.hgrow="ALWAYS" />
                              <TextField fx:id="txtLanguage" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label fx:id="lblGenre" text="Genre" />
                              <Region HBox.hgrow="ALWAYS" />
                              <TextField fx:id="txtGenre" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label fx:id="lblISBN" text="ISBN" />
                              <Region HBox.hgrow="ALWAYS" />
                              <TextField fx:id="txtISBN" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox alignment="TOP_CENTER" prefHeight="195.0" prefWidth="232.0" spacing="10.0">
                     <children>
                        <Label text="DVD">
                           <font>
                              <Font size="16.0" />
                           </font></Label>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label fx:id="lblDirector" text="Director" />
                              <Region HBox.hgrow="ALWAYS" />
                              <TextField fx:id="txtDirector" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label fx:id="lblDVDLanguage" text="Language" />
                              <Region HBox.hgrow="ALWAYS" />
                              <TextField fx:id="txtLanguage" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label fx:id="lblRuntime" text="Runtime" />
                              <Region HBox.hgrow="ALWAYS" />
                              <TextField fx:id="txtRuntime" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox alignment="TOP_CENTER" spacing="10.0">
                     <children>
                        <Label text="Laptop">
                           <font>
                              <Font size="16.0" />
                           </font></Label>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label fx:id="lblMake" text="Manufacturer" />
                              <Region HBox.hgrow="ALWAYS" />
                              <TextField fx:id="txtMake" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label fx:id="lblModel" text="Model" />
                              <Region HBox.hgrow="ALWAYS" />
                              <TextField fx:id="txtModel" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label fx:id="lblOS" text="Operating System" />
                              <Region HBox.hgrow="ALWAYS" />
                              <TextField fx:id="txtOS" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding></VBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Button fx:id="btnUpdate" minWidth="75.0" mnemonicParsing="false" prefWidth="115.0" text="Update Resource" />
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="btnDelete" minWidth="75.0" mnemonicParsing="false" prefWidth="115.0" text="Delete Resource" />
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="btnExit" minWidth="75.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="115.0" text="Exit" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
