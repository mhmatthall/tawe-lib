<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>

<BorderPane minHeight="145.0" minWidth="250.0" prefHeight="175.0" prefWidth="376.0" xmlns="http://javafx.com/javafx/8.0.192" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SearchUserController">
   <top>
      <Label fx:id="lblTitle" text="Search for User" BorderPane.alignment="CENTER">
         <font>
            <Font size="23.0" />
         </font></Label>
   </top>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <center>
      <HBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Label prefHeight="17.0" prefWidth="79.0" text="User ID:" />
            <TextField fx:id="txtUserID" onAction="#searchUser" prefHeight="25.0" prefWidth="176.0" />
         </children>
      </HBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnSearch" minWidth="75.0" mnemonicParsing="false" onAction="#searchUser" prefHeight="25.0" prefWidth="75.0" text="Search" />
            <Region prefHeight="25.0" prefWidth="99.0" />
            <Button fx:id="btnCancel" minHeight="25.0" minWidth="75.0" mnemonicParsing="false" onAction="#close" prefHeight="25.0" prefWidth="75.0" text="Cancel" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
